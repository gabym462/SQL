#Use a select statement to order the items by price.
select *
from superstore
order by price desc;

#Find the maximum spent from the date.
select max(price)
from superstore;

#Use a select statement to find the total price spent on items in catergory Kitchen Supplies.
select sum(price)
from superstore
where category = "Kitchen Supplies";

#Use a select statement to find stock quantity for items named Smart LED TV. 
select stock_quantity
from superstore
where item_name = "Smart LED TV";
